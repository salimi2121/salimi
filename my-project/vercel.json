{
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/assets/.*",
      "dest": "/assets/__CODE_BLOCK_0__"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}